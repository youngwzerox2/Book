<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- *********************** dao interface 경로+이름 -->
<mapper namespace="com.example.dao.KakaoLibraryDAO">

    <!-- ****************************** 도서관 위치 ******************************** -->
    <!-- 도서관 위치 전체리스트 -->
    <select id="getAllLocations" resultType="KakaoLibrary">
        SELECT *
        FROM library
        WHERE library_region = '23'
    </select>

</mapper>
